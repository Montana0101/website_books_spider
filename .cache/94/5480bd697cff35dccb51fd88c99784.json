 \r\nconst Home = () => {\r\n    const [list,getList] = useState([])\r\n    // const [keyword,getKeyword] = useState('')\r\n\r\n    useEffect(()=>{\r\n    //  调用\r\n    },[])\r\n    \r\n    const getBooks = async (keyword) => {\r\n        let {data} = await axios.get(`http://127.0.0.1:7777/api/spider/books?keyword=${keyword}`)\r\n        getList(data)\r\n        console.log(data.splice(0,10))\r\n      \r\n    }\r\n    const onSearch = value => {\r\n        getBooks(value)\r\n    };\r\n\r\n\r\n    return (\r\n        <div>\r\n            <header>\r\n                <Search\r\n                    placeholder=\"input search text\"\r\n                    allowClear\r\n                    enterButton=\"Search\"\r\n                    size=\"large\"\r\n                    onSearch={onSearch}\r\n                />\r\n            </header>\r\n\r\n            <main>\r\n                <List\r\n                    itemLayout=\"horizontal\"\r\n                    dataSource={list}\r\n                    renderItem={item => (\r\n                        <a href={item.url} className=\"list\" target=\"_blank\">\r\n                             <img src={item.img} style={{height:'100px',width:'100px'}}/>\r\n                             <div className='list-item'>\r\n                                 <div className='list-item-title'>\r\n                                     <span>{item.title}  </span> \r\n                                     <span>{item.author}</span>\r\n                                    </div>\r\n                                 <div className='list-item-col'>\r\n                                     <span>¥{item.price}</span>\r\n                                     <span>{item.store}</span>\r\n                                    <span>{item.source}</span>\r\n                          \r\n                                 </div>\r\n                             </div>\r\n                        </a>\r\n                    )}\r\n                />\r\n            </main>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Home"},"lineCount":null}},"error":null,"hash":"61cf9e7d91292fedbb9762d9b881ca2a","cacheData":{"env":{}}}